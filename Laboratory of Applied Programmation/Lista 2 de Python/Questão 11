num = []
for i in range (1, 3, 1):
    num.append (int(input("Digite um número: ")))
soma = 0
print (f"Os números inteiros entre {num[0]} e {num[1]} são:\n")
if num[0] < num[1]:
    for i in range (num[0], num[1] + 1, 1):
        print (i)
        soma = soma + i
elif num[1] < num[0]:
    for i in range (num[1], num [0] + 1, 1):
        print (i)
        soma = soma + i
print (f"\nA soma entre todos esses números é: {soma}")
