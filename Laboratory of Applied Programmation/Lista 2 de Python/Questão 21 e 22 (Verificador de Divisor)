n = int(input("Digite um número: "))
aux = 0
divide = []
for i in range (n):
    if n%(i+1) == 0:
        aux += 1
        divide.append(i + 1)
if aux == 2:
    print (f"{n} é um número primo, ou seja, é divisível por 1 e por ele mesmo")
else:
    print (f"{n} não é um número prima, já que é divisível por {divide}")
