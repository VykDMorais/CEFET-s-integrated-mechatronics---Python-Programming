num = int(input("\nDigite um número: "))
primo = []
divide = 0

for i in range(1, num + 1, 1):
    if (i%2 != 0 and i%3 != 0 and i%5 != 0 and i%7 != 0) or i == 2 or i == 3 or i == 5 or i == 7:
        if i != 1:
            primo.append(i)
    divide += 5
print(f"Números primos: {primo}")
print(f"Número de divisões: {divide}")
