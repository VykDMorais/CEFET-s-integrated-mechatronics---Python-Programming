import math
num = int(input("\nDigite um número: "))
primo = []
divide = 0
a = 0
b = 0
for b in range(1, num + 1, 1):
    aux = b**(1/2)
    c = aux.is_integer()
    if c == True and aux%5 != 0:
        primo.insert(a, b)
        a += 1
    divide += 4
    erro -> 143 e 187
    
print(f"Números primos: {primo}")
print(f"Número de divisões: {divide}")

#primo.insert (a, i) -> com 'a' sendo um auxiliar q sobe a cada primo verdadeiro
